{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#titulo-insta",
      "flex-layout.row#insta",
      "flex-layout.row#seo",
      "flex-layout.row#content-footer-general"
    ]
  },

  "footer-layout.mobile": {
    "children": [
      "flex-layout.row#titulo-insta",
      "flex-layout.row#insta",
      "flex-layout.row#seo",
      "flex-layout.row#content-footer-general"
    ],
    "props": {
      "blockClass": "mainrow-footer-mobile",
      "fullWidth": true
    }
  },

  "flex-layout.row#club-pillin": {
    "children": [
      "flex-layout.col#title-text-pillin",
      "flex-layout.col#banners-club-pillin"
    ],
    "props": {
      "blockClass": "clubPillin",
      "horizontalAlign": "center"
    }
  },

  "flex-layout.col#title-text-pillin": {
    "children": [
      "rich-text#title-club",
      "rich-text#text-club",
      "newsletter-form"
    ],
    "props": {
      "blockClass": "colTitleTextPillin",
      "width": "30%",
      "preventVerticalStretch": true,
      "verticalAlign": "middle"
    }
  },

  "flex-layout.col#banners-club-pillin": {
    "children": [
      "flex-layout.row#row-banners",
      "flex-layout.row#row-button"
    ],
    "props": {
      "blockClass": "colBannersPillin",
      "width": "70%",
      "preventVerticalStretch": true,
      "verticalAlign": "middle",
      "paddingLeft": 10
    }
  },

  "flex-layout.row#row-banners": {
    "children": [
      "image#club-pillin-1",
      "image#club-pillin-2",
      "image#club-pillin-3"
    ],
    "props": {
      "blockClass": "rowBanners"
    }
  },

  "flex-layout.row#row-banners-mobile": {
    "children": [
      "image#club-pillin-1",
      "image#club-pillin-2",
      "image#club-pillin-3"
    ],
    "props": {
      "blockClass": "rowBanners-mobile",
      "colSizing": "auto"
    }
  },

  "flex-layout.row#row-button": {
    "children": [
      "rich-text#ver-mas-pillin"
    ],
    "props": {
      "blockClass": "rowButton"
    }
  },


  "form#club-pillin-footer": {
    "props": {
      "entity": "clients",
      "schema": "person"
    },
    "children": [
      "form-input.text#firstNameClubPillin",
      "form-input.text#lastNameClubPillin",
      "form-field-group#addressClubPillin",
      "form-input.checkbox#agreementClubPillin",
      "form-submit#ClubPillin"
    ],
    "blocks": ["form-success#ClubPillin"]
  },

  "form-success#ClubPillin": {
    "children": [
      "rich-text#successSubmit"
    ]
  },

  "rich-text#successSubmit": {
    "props": {
      "text": "Succesfully submitted the data!",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },

  "form-input.text#firstNameClubPillin": {
    "props": {
      "pointer": "#/properties/firstName"
    }
  },

  "form-input.text#lastNameClubPillin": {
    "props": {
      "pointer": "#/properties/lastName"
    }
  },

  "form-input.checkbox#agreementClubPillin": {
    "props": {
      "pointer": "#/properties/agreement",
      "label": "Do you agree that this is the best form component in the whole wide world?"
    }
  },

  "form-field-group#addressClubPillin": {
    "props": {
      "pointer": "#/properties/address"
    }
  },

  "form-submit#ClubPillin": {
    "props": {
      "label": "Submit"
    }
  },


  "newsletter-form": {
    "children": [
      "newsletter-input-email",
      "newsletter-submit"
    ]
  },

  "rich-text#title-club": {
    "props": {
      "blockClass": "cp-titleClub",
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "### Únete a nuestro Club Pillin"
    }
  },

  "rich-text#text-club": {
    "props": {
      "blockClass": "cp-textClub",
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "Al unirte obtendrás un 10% Dcto en tu primera compra. Además accedes a varios beneficios"
    }
  },

  "flex-layout.row#club-pillin-mobile": {
    "children": [
      "flex-layout.col#title-text-pillin",
      "flex-layout.col#banners-club-pillin-mobile"
    ],
    "props": {
      "blockClass": "clubPillin-mobile"
    }
  },

  "flex-layout.col#banners-club-pillin-mobile": {
    "children": [
      "flex-layout.row#row-banners-mobile",
      "flex-layout.row#row-button"
    ],
    "props": {
      "blockClass": "colBannersPillin-mobile",
      "verticalAlign": "middle"
    }
  },



  "newsletter": {
    "props": {
      "blockClass": "cp-news"
    }
  },

  "image#club-pillin-1": {
    "props": {
      "blockClass": "cp-image1",
      "link": "/",
      "src": "assets/img/club-image-01.jpg",
      "alt": "Club Pillin Promo 1",
      "maxWidth": "100%",
      "width": "100%"
    }
  },

  "image#club-pillin-2": {
    "props": {
      "blockClass": "cp-image2",
      "link": "/",
      "src": "assets/img/club-image-02.jpg",
      "alt": "Club Pillin Promo 2",
      "maxWidth": "100%",
      "width": "100%"
    }
  },

  "image#club-pillin-3": {
    "props": {
      "blockClass": "cp-image3",
      "link": "/",
      "src": "assets/img/club-image-03.jpg",
      "alt": "Club Pillin Promo 3",
      "maxWidth": "100%",
      "width": "100%"
    }
  },





  "rich-text#ver-mas-pillin": {
    "props": {
      "blockClass": "cp-buttonVerMas",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "[Conoce más](#)"
    }
  },


  // Instagram
  "flex-layout.row#titulo-insta": {
    "children": [
      "rich-text#titulo_insta"
    ],
    "props": {
      "blockClass": "titulo-row-insta"
    }
  },

  "flex-layout.row#insta": {
    "children": [
      "sandbox.product",
      "iframe"
    ],
    "props": {
      "fullWidth": true,
      "blockClass": "rowInsta"
    }
  },

  "sandbox.product": {
    "props": {
      "initialContent": "<script src='https://cdn.lightwidget.com/widgets/lightwidget.js'></script>",
      "allowCookies": true,
      "blockClass": "scriptInsta",
      "height": "0px"
    }
  },

  "rich-text#titulo_insta": {
    "props": {
      "blockClass": "titulo-insta",
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "Síguenos en Instagram: [@PillinChile](https://www.instagram.com/pillinchile/)"
    }
  },

  "iframe": {
    "props": {
      "width": "100%",
      "height": "350px",
      "src": "//lightwidget.com/widgets/f0b9d22644985bb191865517469b9900.html",
      "blockClass": "iframeInsta"
    }
  },


  //SEO
  "flex-layout.row#seo": {
    "children": [
