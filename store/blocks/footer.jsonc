{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#club-pillin",
      "flex-layout.row#seo",
      "flex-layout.row#content-footer-general"
    ]
  },
  "footer-layout.mobile": {
    "children": [
      "flex-layout.row#footer-mobile"
    ]
  },
  "flex-layout.row#footer-mobile": {
    "children": [
      // "social-networks",
      // "accepted-payment-methods",
      // "powered-by"
      "flex-layout.row#content-footer-general"
    ]
  },

  "flex-layout.row#club-pillin": {
    "children": [
      "flex-layout.col#title-text-pillin",
      "flex-layout.col#banners.club-pillin"
    ],
    "props": {
      "blockClass": "clubPillin",
      "horizontalAlign": "center"
    }
  },

  "flex-layout.col#title-text-pillin": {
    "children": [
      "rich-text#title-club",
      "rich-text#text-club",
      "newsletter-form"
    ],
    "props": {
      "blockClass": "colTitleTextPillin",
      "width": "30%",
      "preventVerticalStretch": true,
      "verticalAlign": "middle"
    }
  },

  "flex-layout.col#banners.club-pillin": {
    "children": [
      "flex-layout.row#row-banners",
      "flex-layout.row#row-button"
    ],
    "props": {
      "blockClass": "colBannersPillin",
      "width": "70%",
      "preventVerticalStretch": true,
      "verticalAlign": "middle",
      "paddingLeft": 10
    }
  },

  "flex-layout.row#row-banners": {
    "children": [
      "image#club-pillin-1",
      "image#club-pillin-2",
      "image#club-pillin-3"
    ],
    "props": {
      "blockClass": "rowBanners"
    }
  },

  "flex-layout.row#row-button": {
    "children": [
      "rich-text#ver-mas-pillin"
    ],
    "props": {
      "blockClass": "rowButton"
    }
  },

  "newsletter-form": {
    "children": [
      "newsletter-input-email",
      "newsletter-submit"
    ]
  },

  "rich-text#title-club": {
    "props": {
      "blockClass": "cp-titleClub",
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "### Únete a nuestro Club Pillin"
    }
  },

  "rich-text#text-club": {
    "props": {
      "blockClass": "cp-textClub",
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "Al unirte obtendrás un 10% Dcto en tu primera compra. Además accedes a varios beneficios"
    }
  },

  "newsletter": {
    "props": {
      "blockClass": "cp-news"
    }
  },

  "image#club-pillin-1": {
    "props": {
      "blockClass": "cp-image1",
      "link": "/",
      "src": "assets/img/club-image-01.png",
      "alt": "Club Pillin Promo 1",
      "maxWidth": "100%",
      "width": "100%"
    }
  },

  "image#club-pillin-2": {
    "props": {
      "blockClass": "cp-image2",
      "link": "/",
      "src": "assets/img/club-image-02.png",
      "alt": "Club Pillin Promo 2",
      "maxWidth": "100%",
      "width": "100%"
    }
  },

  "image#club-pillin-3": {
    "props": {
      "blockClass": "cp-image3",
      "link": "/",
      "src": "assets/img/club-image-03.png",
      "alt": "Club Pillin Promo 3",
      "maxWidth": "100%",
      "width": "100%"
    }
  },

  "rich-text#ver-mas-pillin": {
    "props": {
      "blockClass": "cp-buttonVerMas",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "[Conoce más](#)"
    }
  },


  //SEO
  "flex-layout.row#seo": {
    "children": [
      "rich-text#categorias-mas-buscadas",
      "rich-text#cat-1"
    ],
    "props": {
      "blockClass": "rowSeo",
      "colSizing": "auto"
    }
